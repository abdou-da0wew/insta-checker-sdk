{
  "name": "insta-checker-sdk",
  "version": "1.0.6",
  "description": "A robust, production-ready Instagram username availability checker written in TypeScript.",
  "author": "Abdou-da0wew <aabdouaydev911@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && bun x tsc && bun build ./src/index.ts --outdir ./dist --target node --format cjs && mv dist/index.js dist/index.cjs",
    "test": "bun test",
    "test:live": "RUN_LIVE=true bun test",
    "lint": "bun x tsc --noEmit",
    "prepublishOnly": "bun run build",
    "release": "npm version patch && git push origin main --tags"
  },
  "keywords": [
    "instagram",
    "username",
    "checker",
    "availability",
    "bot",
    "discord",
    "api",
    "typescript",
    "bun"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abdou-da0wew/insta-checker-sdk.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "^1.3.8",
    "typescript": "^5.0.0"
  }
}
